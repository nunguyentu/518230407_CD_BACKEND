<script>
   function changeStatus(orderId, status) {
      $.post("http://localhost:5001/orders/updateStatusDelivering",
        {
         orderId: orderId,
        },
        function (data){
         const {success, message} = data;
         
         if(success=== true){
            window.location.href = "/orders";
         }else{
            alert(message);
         }
        }
      )
   }
</script>